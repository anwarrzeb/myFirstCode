class Animal:
    color = "red"
    type = "qurbani"
    location = "Karchi"
    price_increase = 2.5
    def __init__(self, name, age, price):
        self.name = name
        self.age = age
        self.price = price
    def cal_price_increa(self):
        priceincreae = self.price * 2.5
        return priceincreae

Animal1 =  Animal("goat", 4, 5000)
Animal2 = Animal("cow", 8, 9000)
print(Animal2. color)
print(Animal1.color)
print(Animal2.location)
print(Animal2.price)
print(Animal2.cal_price_increa())
